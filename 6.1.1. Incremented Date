d = int(input())
m = int(input())
y = int(input())

if m in [1, 3, 5, 7, 8, 10, 12]:
    lim = 31
elif m in [4, 6, 9, 11]:
    lim = 30
elif m == 2:
    if (y % 400 == 0) or (y % 4 == 0 and y % 100 != 0):
        lim = 29
    else:
        lim = 28
else:
    lim = 0

if d < 1 or d > lim or m < 1 or m > 12 or y < 1:
    print("Invalid Date")
else:
    d += 1
    if d > lim:
        d = 1
        m += 1
        if m > 12:
            m = 1
            y += 1
    print(f"{d:02d}-{m:02d}-{y}")

